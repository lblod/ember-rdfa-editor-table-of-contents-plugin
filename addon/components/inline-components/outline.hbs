<ul>
  {{#each @entries as |outlineEntry|}}
    <li>
      <AuButton
        @skin='link'
        {{on 'click' (fn @onEntryClick outlineEntry.node)}}
      >
        {{outlineEntry.content}}

      </AuButton>
      {{#if outlineEntry.children}}
        <InlineComponents::Outline @entries={{outlineEntry.children}} />
      {{/if}}
    </li>
  {{/each}}
</ul>
