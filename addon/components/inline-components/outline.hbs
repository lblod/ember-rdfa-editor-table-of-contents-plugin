<ul>
  {{#each @entries as |outlineEntry|}}
    <li>
      <AuButton @skin='link' onclick={{(fn @onEntryClick outlineEntry.node)}}>
        {{outlineEntry.content}}

      </AuButton>
      {{#if outlineEntry.children}}
        <InlineComponents::Outline
          @entries={{outlineEntry.children}}
          @onEntryClick={{@onEntryClick}}
        />
      {{/if}}
    </li>
  {{/each}}
</ul>
